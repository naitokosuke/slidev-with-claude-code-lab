{
  "name": "slidev-monorepo",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:my-own-theme": "pnpm --filter slidev-theme-deep-sea dev",
    "dev:self-intro": "pnpm --filter self-intro dev",
    "dev:teach-me-about-vue-router": "pnpm --filter teach-me-about-vue-router dev",
    "build:my-own-theme": "pnpm --filter slidev-theme-deep-sea build",
    "build:self-intro": "pnpm --filter self-intro build",
    "build:teach-me-about-vue-router": "pnpm --filter teach-me-about-vue-router build",
    "export:my-own-theme": "pnpm --filter slidev-theme-deep-sea export",
    "export:self-intro": "pnpm --filter self-intro export",
    "export:teach-me-about-vue-router": "pnpm --filter teach-me-about-vue-router export"
  },
  "devDependencies": {
    "@slidev/cli": "^52.1.0",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "vue": "^3.5.18"
  },
  "packageManager": "pnpm@8.15.1"
}
